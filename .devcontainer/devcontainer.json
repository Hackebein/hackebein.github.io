{
	"customizations": {
		"vscode": {
			"extensions": [
				"yzhang.markdown-all-in-one",
				"streetsidesoftware.code-spell-checker",
				"DavidAnson.vscode-markdownlint",
				"shd101wyy.markdown-preview-enhanced",
				"bierner.github-markdown-preview"
			]
		}
	},
	"forwardPorts": [3000],
	"portsAttributes": {
		"3000": {
			"label": "xlog-bin"
		}
	},
	"postCreateCommand": "go install github.com/emad-elsaid/xlog/cmd/xlog@latest;docker run --rm -it -v \"$(pwd)/dns:/dns\" ghcr.io/stackexchange/dnscontrol write-types"
}
